<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainnav"
    app:startDestination="@id/alertFragment">

    <fragment
        android:id="@+id/alertFragment"
        android:name="com.example.weatherapplication.AlertFragment"
        android:label="fragment_alert"
        tools:layout="@layout/fragment_alert" />
    <fragment
        android:id="@+id/favFragment"
        android:name="com.example.weatherapplication.FavFragment"
        android:label="fragment_fav"
        tools:layout="@layout/fragment_fav" />
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.weatherapplication.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <argument
            android:name="long"
            app:argType="float" />
        <argument
            android:name="lat"
            app:argType="float" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.weatherapplication.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <argument
            android:name="Longitude"
            app:argType="float" />
        <argument
            android:name="Latitude"
            app:argType="float" />
        <action
            android:id="@+id/action_settingFragment_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
</navigation>